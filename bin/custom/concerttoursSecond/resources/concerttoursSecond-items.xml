<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="items.xsd">

    <!--Created collection-->
    <collectiontypes>
        <collectiontype code="StringList" elementtype="java.lang.String" type="collection"/>
    </collectiontypes>

    <enumtypes>
        <!--dynamic-->
        <enumtype code="CustomMusicType" generate="true" autocreate="true" dynamic="true">
        </enumtype>

        <!--non-dynamic-->
        <enumtype code="CustomConcertType" generate="true" autocreate="true" dynamic="false">
            <value code="openair"/>
            <value code="indoor"/>
        </enumtype>
    </enumtypes>

    <!--Created map-->
    <maptypes>
        <maptype code="NameMapType" argumenttype="java.lang.String" returntype="java.lang.String"/>
    </maptypes>

    <!--Relations-->
    <relations>
        <!--1-m-->
        <relation code="CustomProduct2RockBand" localized="false" generate="false" autocreate="true">
            <sourceElement qualifier="tours" type="CustomProduct" collectiontype="set">
                <modifiers read="true" write="true" search="true" optional="true"/>
            </sourceElement>
            <targetElement qualifier="customBand" type="CustomBand" cardinality="one">
                <modifiers read="true" write="true" search="true" optional="true"/>
            </targetElement>
        </relation>

        <!--m-n-->
        <relation code="CustomBand2MusicType" localized="false" autocreate="true" generate="false">
            <deployment table="CustomBand2MusicType" typecode="30369"/>
            <sourceElement type="CustomBand" qualifier="customBands" collectiontype="set" cardinality="many" ordered="false">
                <modifiers read="true" write="true" search="true" optional="true"/>
            </sourceElement>
            <targetElement type="CustomMusicType" cardinality="many" qualifier="customTypes">
                <modifiers read="true" write="true" search="true" optional="true"/>
            </targetElement>
        </relation>

        <!--Relation for superItem 1-m-->
        <relation code="SuperItemConcertType" localized="false">
            <sourceElement type="SuperItem" qualifier="superItem" cardinality="one" ordered="false">
                <modifiers read="true" write="true" search="true" optional="true"/>
            </sourceElement>
            <targetElement type="CustomConcertType" qualifier="types" cardinality="many" ordered="false">
                <modifiers read="true" write="true" search="true" optional="true"/>
            </targetElement>
        </relation>
    </relations>

    <!--There items-->
    <itemtypes>
        <!--Item with collection-->
        <itemtype code="Author" generate="true" autocreate="true">
            <deployment table="Author" typecode="10107"/>
            <attributes>
                <attribute qualifier="tracks" type="StringList">
                    <description>All tracks of this author</description>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>

        <!--With map-->
        <itemtype code="Track" generate="true" autocreate="true">
            <deployment table="Track" typecode="10106"/>
            <attributes>
                <attribute qualifier="nameMap" type="NameMapType">
                    <description>Map defines author - track</description>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="CustomProduct" extends="Product" generate="true" autocreate="true">
            <attributes>
                <attribute qualifier="customHashtag" type="java.lang.String">
                    <description>hashtag for sc media just for concerttoursSecond</description>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="CustomBand" generate="true" autocreate="true">
            <deployment table="CustomBands" typecode="10105"/>
            <attributes>
                <attribute qualifier="code" type="java.lang.String">
                    <description>code of band</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="name" type="java.lang.String">
                    <description>name of band</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="history" type="java.lang.String">
                    <description>history of band</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="albumSales" type="java.lang.Long">
                    <description>amount of sales</description>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="CustomVenue" generate="true" autocreate="true">
            <deployment table="Venue2" typecode="10102"/>
            <attributes>
                <attribute qualifier="name" type="java.lang.String">
                    <description>name of the street</description>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>

        <!--1-1 relation Venue - Concert-->

        <itemtype code="CustomConcert" generate="true" extends="VariantProduct" autocreate="true">
            <attributes>
                <attribute qualifier="venue" type="CustomVenue">
                    <description>one to one Concert-Venue</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="date" type="java.util.Date">
                    <description>date of concert</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="concertType" type="CustomConcertType">
                    <description>type of concert (indoors or open air)</description>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>


        <!--Item with all possible attributes types-->
        <itemtype code="SuperItem" extends="GenericItem" generate="true" autocreate="true">
            <deployment table="SuperItem" typecode="20103"/>
            <attributes>
                <attribute qualifier="code" type="java.lang.String">
                    <description>code of superItem</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="band" type="CustomBand">
                    <description>Just a band which we created before</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="musicType" type="CustomMusicType">
                    <description>Music type we created before</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="stringList" type="StringList">
                    <description>List of some strings</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="nameMap" type="NameMapType">
                    <description>map of name to name</description>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype generate="true"
                  code="ContactRequest"
                  jaloclass="org.training.jalo.ContactRequest"
                  extends="GenericItem"
                  autocreate="true">
            <deployment table="ContactRequest" typecode="10103"/>
            <attributes>
                <attribute qualifier="message" type="java.lang.String">
                    <description>Message</description>
                    <modifiers initial="true"/>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="sender" type="java.lang.String">
                    <description>Sender</description>
                    <modifiers initial="true"/>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>

    </itemtypes>
</items>
